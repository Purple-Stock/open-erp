<section class="section">
  <div class="section-header d-mobile-none">
    <h1>Shein Orders</h1>
    <div class="section-header-breadcrumb">
      <div class="breadcrumb-item"><%= link_to 'Dashboard', root_path %></div>
      <div class="breadcrumb-item"><%= link_to 'Shein Orders', shein_orders_path %></div>
      <div class="breadcrumb-item">Show Shein Order</div>
    </div>
  </div>

  <div class="section-body">
    <div class="card">
      <div class="card-header">
        <h4>Show Shein Order</h4>
      </div>
      <div class="card-body">
        <dl class="row">
          <dt class="col-sm-3">Data:</dt>
          <dd class="col-sm-9"><%= @shein_order.data %></dd>
        </dl>
      </div>
    </div>
  </div>
</section>

<%= link_to icon('fas fa-edit', 'Edit'), edit_shein_order_path(@shein_order), class: 'btn btn btn-icon icon-left btn-warning' %>
<%= link_to icon('fas fa-arrow-left', 'Back'), shein_orders_path, class: 'btn btn-icon icon-left btn-secondary d-mobile-none' %>

<% content_for(:page_title, 'Show Shein Order') %>
