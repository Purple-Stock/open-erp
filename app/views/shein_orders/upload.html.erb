<!-- app/views/shein_orders/upload.html.erb -->
<br><br>
<h1>Atualizar dados pedidos Shein com planilha</h1>
<%= form_with(url: import_shein_orders_path, local: true, method: :post, multipart: true) do %>
  <div class="row">
    <div class="col-md-12">
      <%= file_field_tag :file, accept: '.xlsx, .xls, .zip' %>
    </div>  
    <div class="col-md-12">
      <%= submit_tag "Upload", class: 'btn btn-primary mb-3' %>
    </div>
  </div>
<% end %>
